{
	"name": "vue-toy",
	"version": "0.4.1",
	"description": "Vue toy",
	"main": "cjs/Vue.js",
	"module": "esm/Vue.js",
	"types": "lib/Vue.d.ts",
	"files": [
		"cjs",
		"esm",
		"lib",
		"style",
		"*.md"
	],
	"scripts": {
		"docs": "packez build ./examples/index.js -d docs -c",
		"start": "tsc -w",
		"cjs": "transform-es src -d cjs -c -t cjs",
		"esm": "transform-es src -d esm -c -t esm",
		"tsc": "tsc",
		"demo": "packez server ./examples/index.js -d dist -w -c",
		"clear": "rimraf lib cjs esm docs dist",
		"build": "run-s clear tsc cjs esm docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bplok20010/vue-toy.git"
	},
	"keywords": [
		"vue",
		"vue-toy"
	],
	"author": "nobo.zhou@foxmail.com",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/bplok20010/vue-toy/issues"
	},
	"homepage": "https://github.com/bplok20010/vue-toy#readme",
	"devDependencies": {
		"@types/lodash": "^4.14.155",
		"@types/react": "^16.9.36",
		"@types/react-dom": "^16.9.8",
		"npm-run-all": "^4.1.5",
		"packez": "^4.1.3",
		"rimraf": "^3.0.2",
		"transform-es": "^5.0.4"
	},
	"browserslist": [
		">=0.25%",
		"not dead",
		"not op_mini all",
		"not Android 4.4.3-4.4.4",
		"not ios_saf < 10",
		"not Chrome < 50",
		"firefox ESR"
	],
	"dependencies": {
		"classnames": "^2.2.6",
		"lodash": "^4.17.15",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"shallowequal": "^1.1.0"
	}
}
